    % , gen.          .meta:n             = { variant = Genitiv   }
    % , Gen           .meta:n             = { variant = Genitiv   }
    % , Gen.          .meta:n             = { variant = Genitiv   }
    % , dat           .meta:n             = { variant = Dativ     }
    % , dat.          .meta:n             = { variant = Dativ     }
    % , Dat           .meta:n             = { variant = Dativ     }
    % , Dat.          .meta:n             = { variant = Dativ     }
    % , akk           .meta:n             = { variant = Akkusativ }
    % , akk.          .meta:n             = { variant = Akkusativ }
    % , Akk           .meta:n             = { variant = Akkusativ }
